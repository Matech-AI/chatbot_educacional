# Configurações da API
OPENAI_API_KEY=your_openai_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
NVIDIA_API_KEY=your_nvidia_api_key_here
GOOGLE_DRIVE_API_KEY=your_google_drive_api_key_here

# Configurações de Embeddings
PREFER_OPEN_SOURCE_EMBEDDINGS=true
OPEN_SOURCE_EMBEDDING_MODEL=intfloat/multilingual-e5-large

# Configurações do servidor RAG
RAG_SERVER_URL=http://rag-server:8001
CHROMA_PERSIST_DIR=/app/data/.chromadb
MATERIALS_DIR=/app/data/materials

# Configurações de autenticação
JWT_SECRET_KEY=your_jwt_secret_key_here
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Configurações de CORS
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000,https://iadnadaforca.com.br,https://www.iadnadaforca.com.br

# Configurações de banco de dados (se usar)
DATABASE_URL=postgresql://user:password@localhost/dbname

# Configurações de logging
LOG_LEVEL=INFO

# Configurações do Redis (opcional)
REDIS_URL=redis://redis:6379

# Configurações de Email
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USERNAME=your_email@gmail.com
EMAIL_PASSWORD=your_app_password
EMAIL_FROM=your_email@gmail.com

# Configurações específicas para Hostinger
HOSTINGER=false
SERVER_IP=0.0.0.0
RAG_PORT=8000
API_PORT=8001
FRONTEND_PORT=3000

# Caminhos específicos para Hostinger
HOSTINGER_CHROMADB_PATH=/root/dna-forca-complete/backend/data/.chromadb
HOSTINGER_MATERIALS_PATH=/root/dna-forca-complete/backend/data/materials